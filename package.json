        "prettier-format": "prettier --config .prettierrc ./src/**/*.ts --write",
        "dev": "node ophidian.config.mjs dev",
        "build": "node ophidian.config.mjs production"
    },
    "keywords": [],
    "author": "",
    "license": "MIT",
    "devDependencies": {
        "@eslint/js": "^9.39.2",
        "@ophidian/build": "^1.2.1",
        "@types/jest": "^29.5.14",
